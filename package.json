    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "react-test-renderer": "^18.3.1",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
